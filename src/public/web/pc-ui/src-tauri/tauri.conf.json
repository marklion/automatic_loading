{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "pc-ui",
  "version": "0.1.0",
  "identifier": "d8sis.cn",
  "build": {
    "frontendDist": "../dist",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "pc-ui",
        "resizable": true,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "webviewInstallMode":{
        "type":"offlineInstaller"
      }
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
